{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "vitest run",
    "db:generate": "prisma generate --schema ../prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --name init --schema ../prisma/schema.prisma",
    "db:deploy": "prisma migrate deploy --schema ../prisma/schema.prisma",
    "db:seed": "prisma db seed --schema ../prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.4",
    "prisma": "^5.19.1",
    "tsx": "^4.17.0",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  },
  "prisma": {
    "seed": "tsx ../prisma/seed.ts"
  }
}
